root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = true

[*.gradle]
indent_style = tab

[*.java]
indent_style = tab
ij_java_names_count_to_use_import_on_demand = 99

[*.json]
indent_style = tab

[.editorconfig]
indent_style = space
indent_size = 4
